{

    "name": "Malicious Chrome Extension",
    "description": "A malicious chrome extension build for a uni project.",
    "version": "0.1.3",
    "version_name": "0.1.3: A cunning disguise",
    "manifest_version": 3,
    
    "permissions": [
        "activeTab", 
        "scripting"
    ],

    "background": {
        "service_worker": "script/background_bootstrap.js"
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script/content_bootstrap.js"]
        }
    ],

    "action": {
        "default_popup": "index.html"
    }
}